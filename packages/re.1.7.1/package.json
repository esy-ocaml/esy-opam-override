{
  "build": [
    [
      "bash",
      "-c",
      "#{os == 'windows' ? 'patch -p1 < patch-oasis-re.1.7.1-BREAK STUFF' : 'true'}"
    ],
    [
      "ocaml",
      "setup.ml",
      "-configure",
      "--prefix",
      "#{self.install}"
    ],
    [
      "ocaml",
      "setup.ml",
      "-build"
    ]
  ],
  "install": [
    [
      "ocaml",
      "setup.ml",
      "-install"
    ]
  ]
}
